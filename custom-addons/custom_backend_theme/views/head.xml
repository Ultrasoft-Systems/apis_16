<oddo>
    <template id="custom_backend_theme" inherit_id="web.layout"
              name="Custom Brending">
        <xpath expr="//link[@rel='shortcut icon']" position="replace">
            <link type="image/x-icon" rel="shortcut icon" href="/custom_backend_theme/static/src/img/favicon.png"/>
        </xpath>
    </template>
</oddo>